{
  "eslint.validate": ["vue", "javascript", "typescript"],
  "eslint.packageManager": "npm",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": true
  },
  "css.validate": false,
  "tailwindCSS.emmetCompletions": true,
  "editor.tabSize": 2
}